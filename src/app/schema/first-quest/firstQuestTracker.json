{
  "$jsonSchema": {
    "bsonType": "object",
    "required": [
      "_id"
    ],
    "properties": {
      "_id": {
        "bsonType": "objectId",
        "description": "Object ID set to discord user ID"
      },
      "doneRescueCall": {
        "bsonType": "boolean",
        "description": "Whether the support message has been sent"
      },
      "guild": {
        "bsonType": "string",
        "description": "Discord guild of user"
      },
      "step": {
        "bsonType": "string",
        "description": "Current first-quest step of user"
      },
      "timestamp": {
        "bsonType": "double",
        "description": "Timestamp of last update (initial value is 0)"
      }
    }
  }
}